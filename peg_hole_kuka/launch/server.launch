<?xml version="1.0"?>
<launch>

	<arg name="action_server_name"  default="kuka_server" />
	<arg name="path_sensor_model"   value="$(find objects)/meshes/plug/config/X.txt"/>

	<arg name="ft_topic"		default="/ft_sensor/netft_data"/>
	<arg name="classifier_topic"	default="/ft_classifier"/>
	<arg name="F_topic"      	default="/belief_features"/>
	<arg name="gmm_param_path"      default="/home/guillaume/MatlabWorkSpace/peg_in_hole_RL/PolicyModelSaved/PolicyModel_txt/gmm_xhu"/>

	<node name="kuka_server_node" pkg="peg_hole_kuka" type="peg_kuka_server_node" required="true"  output="screen">
		<param name="action_server_name" 		value="$(arg action_server_name)"/>
  		<param name="simulation" 			value="false" />
  		<param name="world_frame" 			value="/world" />
  		<param name="model_dt" 				value="0.01" />
  		<param name="reachingThreshold" 		value="0.01" />
  		<param name="orientationThreshold" 		value="0.02" />
		<param name="path_sensor_model" 		value="$(arg path_sensor_model)"/>
		<param name="ft_topic"				value="$(arg ft_topic)"/>
		<param name="classifier_topic"			value="$(arg classifier_topic)"/>
		<param name="F_topic"      			value="$(arg F_topic)"/>
		<param name="gmm_param_path"      		value="$(arg gmm_param_path)"/>
	</node>
</launch>
